// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Listing {
    id        Int      
    userId String
    name      String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    description String
    price String
}
